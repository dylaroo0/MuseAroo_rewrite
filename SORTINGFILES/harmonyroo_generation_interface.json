{
  "HarmonyRoo Generation Interface": {
    "main_controls": {
      "GENERATE": {
        "action": "generate_version()",
        "description": "Generate new version with current control settings",
        "shortcut": "Spacebar or MIDI pad"
      },
      "QUICK_VARIATIONS": {
        "action": "quick_generate_variations(3)",
        "description": "Generate 3 variations with auto-adjusted controls",
        "shortcut": "Shift+Space"
      },
      "COMPARE": {
        "action": "compare_versions([v1_id, v2_id])",
        "description": "A/B compare selected versions",
        "shortcut": "C"
      },
      "RESTORE": {
        "action": "apply_version(version_id)",
        "description": "Restore controls from selected version",
        "shortcut": "R"
      }
    },
